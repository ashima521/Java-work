package com.qa.garage1;

public class Car extends Vehicle {
	boolean ticket;
	public Car(int speed, boolean ticket) {
		super(speed);
		this.ticket = ticket;
		// TODO Auto-generated constructor stub
	}

	public void door() {
		System.out.println("has 4 doors");
		
	}
	

    @Override
	public float bill() {
	if (ticket) {
    	return 50.00f;
	} else {
    		return 500.00f;
    	}
    }
	}	

