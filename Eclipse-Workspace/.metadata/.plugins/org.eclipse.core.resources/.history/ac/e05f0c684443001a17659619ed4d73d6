package com.qa.garage1;

public class Vehicle {
	int speed = 1;
	public boolean fast() {
		this.speed++;}
	
	public int getSpeed() {
		return speed;}

	public void setSpeed(int speed) {
		this.speed = speed;}
    
	int brake = 1;
	public void slow() {
		this.brake--;}
	
	public int getBrake() {
		return brake;}
	
	public void setBrake(int brake) {
		this.brake = brake;}
	}
