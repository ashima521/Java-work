package com.qa.circus;

public class Elephant {

	String species = "asian";
	String colour = "grey";
	int legs = 3;

public void haveMudBath(String paint) {
	colour = paint;
}	

public Elephant(String mySpecies, String myColour, int myLegs) {
	
	species = mySpecies;
	colour = myColour;
	legs = myLegs;
	
		System.out.println("an elephant is born");
	}
}


