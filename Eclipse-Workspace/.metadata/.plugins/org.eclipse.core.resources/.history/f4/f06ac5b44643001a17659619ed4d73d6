package com.qa.garage1;

public abstract class Vehicle {
	private int speed = 1;
	public void fast() {
		this.speed++;}
	
	public int getSpeed() {
		return this.speed++;}

	public void setSpeed(int speed) {
		this.speed = speed;}
    
	private int brake = 0;
	public void slow() {
		this.brake--;}
	
	public int getBrake() {
		return brake;}
	
	public void setBrake(int brake) {
		this.brake = brake;}
	}
