package com.qa.evolution;

public class RattleSnake {
	private short length;
	
	public short getLength() {
		return length;
	}
	public void setLength(short length) {
		this.length = length;
	}
	public void bite() {
		System.out.println("ouch!");
	}
	public void rattle() {
		System.out.println("sssss");
	}
}
